ocalypse/gallery/src/components/Home.js",[],"/Users/may/Documents/GitHub/apocalypse/gallery/src/components/Gallery.js",["59","60"],"import React, { Component } from \"react\";\nimport Zoom from \"./Zoom.js\"\nimport HorizontalScroller from 'react-horizontal-scroll-container';\nimport \"../style/Gallery.css\";\nimport \"../style/General.css\";\n\nconst MenuItem = ({ type, name, artist, year, dsc, materials, link }) => {\n  if (type === \"image\") {\n    return <div>\n      <Zoom src={link}></Zoom>\n    </div>;\n  } else if (type === \"poetry\") {\n    return <div>\n      <img class=\"poetry-img\" src={link}></img>\n    </div>\n  } else if (type === \"video/mp4\" && link.includes(\"https://www.youtube.com/embed/\")) {\n    return <div>\n    <iframe width=\"800px\" height=\"450px\" src={link} \n    frameborder=\"0\" allow=\"accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture\" \n    allowfullscreen></iframe>\n  </div>;\n\n  } else if (type === \"text\") {\n    return <div className=\"text-container\">\n      <h1 class=\"title-container\">{artist}</h1>\n      <h2 class=\"title-container\">{name}</h2>\n      <h3 class=\"title-container\">{year}</h3>\n      <h4 class=\"title-container\"><em>{materials}</em></h4>\n      <p class=\"text-container\">{dsc}</p>\n    </div>\n  } else if (type ===\"title\") {\n    return <div>\n      <div className=\"entrance-container\">\n        <h1 class=\"center\">{name}</h1>\n        <h2 class=\"center\"><em>Scroll down to enter the gallery</em></h2>\n        <br></br>\n        <h5 class=\"center\">Use the browser side scroll bar if gallery scroll is not showing full view</h5>\n      </div>\n    </div>\n  } else if (type ===\"link\") {\n    return <div>\n    <div className=\"end-container\">\n      <h1 class=\"center\">Next: {name}</h1>\n      <h2 class=\"center\"><a href={link} class=\"white\"><em>Enter here</em></a></h2>\n    </div>\n  </div>\n  } else {\n    return <div>\n      <video width=\"888px\" height=\"550px\" controls>\n        <source src={link} type={type} preload=\"auto\"></source>\n        Your browser does not support the video tag.\n      </video>\n    </div>;\n  }\n\n};\n\nclass Gallery extends Component {\n\n  constructor(props) {\n    super(props);\n    this.state = {\n      list: props.list,\n    }\n  }\n\n\n  render() {\n\n    return (\n      <div className=\"Gallery\">\n        <HorizontalScroller invert sensibility={30}>\n        {this.state.list.map((el) => (\n          <div className=\"gallery-item\">\n            <MenuItem key={el.link} type={el.type} name={el.name} artist={el.artist} year={el.year} dsc={el.dsc} link={el.link} materials={el.materials}/>\n          </div>\n        ))}\n        </HorizontalScroller>\n      </div>\n    );\n  }\n}\nexport default Gallery;\n","/Users/may/Documents/GitHub/apocalypse/gallery/src/components/Navbar.js",["61","62"],"/Users/may/Documents/GitHub/apocalypse/gallery/src/components/Zoom.js",["63"],"import React, { Component } from \"react\";\nimport \"../style/Zoom.css\";\n\nclass Zoom extends Component {\n    constructor(props) {\n        super(props);\n    \n        this.state = {\n            src: props.src,\n            backgroundImage: `url(${props.src})`,\n            backgroundPosition: '0% 0%'\n        }\n    }\n\n\n  handleMouseMove = e => {\n    const { left, top, width, height } = e.target.getBoundingClientRect()\n    const x = (e.pageX - left) / width * 100\n    const y = (e.pageY - top) / height * 100\n    this.setState({ backgroundPosition: `${x}% ${y}%` })\n  }\n\n  render = () =>\n    <figure onMouseMove={this.handleMouseMove} style={this.state}>\n      <img class=\"gallery-img\" src={this.state.src} />\n    </figure>\n}\n\nexport default Zoom;\n\n","/Users/may/Documents/GitHub/apocalypse/gallery/src/components/Climate.js",[],"/Users/may/Documents/GitHub/apocalypse/gallery/src/components/Mental.js",[],"/Users/may/Documents/GitHub/apocalypse/gallery/src/components/Pandemic.js",[],"/Users/